{
  "expo": {
    "name": "360 File Manager",
    "slug": "360-file-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "offline": true,
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/89119924-d4b5-4402-96fd-42cdde3b3b53"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to manage files.",
        "NSDocumentsFolderUsageDescription": "This app needs access to your documents folder to manage files."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "package": "com.filemanager360",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-file-system",
        {
          "filePermission": "Allow $(PRODUCT_NAME) to access your files."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "89119924-d4b5-4402-96fd-42cdde3b3b53"
      }
    },
    "owner": "udojidavid"
  }
}
